  "scripts": {
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && webpack --config config/webpack.prod.js  --progress --profile --bail && npm run sw && npm run clean:wwwroot && ncp dist ../MyBrickset.WebApi/wwwroot",
    "build:aot": "npm run build:aot:prod",
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --profile && npm run sw && npm run clean:wwwroot && ncp dist ../MyBrickset.WebApi/wwwroot",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress --profile --bail && npm run sw && npm run clean:wwwroot && ncp dist ../MyBrickset.WebApi/wwwroot",
    "build": "npm run build:dev",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:wwwroot": "npm run rimraf -- ../MyBrickset.WebApi/wwwroot",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "ncp": "ncp",
    "rimraf": "rimraf",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
